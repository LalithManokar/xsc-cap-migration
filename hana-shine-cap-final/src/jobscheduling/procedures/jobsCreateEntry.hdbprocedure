PROCEDURE "SAP_HANA_DEMOCONTENT_EPM_JOBSCHEDULING_PROCEDURES_JOBSCREATEENTRY" (IN DESCRIPTION NVARCHAR(30)) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	AS
BEGIN 

declare DESCNAME string;
DESCNAME := DESCRIPTION;

/*This Procedure inserts a record with the current timestamp into the "JOBSDEMO" table*/

   insert into "SAP_HANA_DEMOCONTENT_EPM_DATA_JOBSDEMO_DETAILS" 
             values (now(), DESCNAME);

END;